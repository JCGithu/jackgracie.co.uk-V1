  "main": "index.js",
  "scripts": {
    "css": "postcss ./configs/tailwindFull.css -o ./assets/css/tailwind.css --config ./configs/postcss.config.js",
    "dev:sinon": "browserify ./assets/nodejs/sinon/sinon.js -o ./assets/js/sinon/sinon.js",
    "build:sinon": "browserify -p tinyify ./assets/nodejs/sinon/sinon.js -o ./assets/js/sinon/sinon.js",
    "dev": "set NODE_ENV=development&&npm run css && npm run dev:sinon",
    "build": "set NODE_ENV=production&& npm run css && npm run build:sinon"
  },
